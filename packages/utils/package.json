    "@jest/globals": "^29.7.0",
    "@meer-js/types": "workspace:*",
    "@types/node": "*",
    "jest": "29.7.0",
    "rimraf": "6.0.1",
    "ts-jest": "29.1.5",
    "typescript": "^5.6.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/utils/src/index.js",
